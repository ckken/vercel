{
  "name": "nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "ncc:run": "ncc run ./nextstart.js",
    "ncc": "yarn rm && ncc build ./nextstart.js -o ./build -m",
    "pkg": "pkg -t host build/index.js -o ./next-start",
    "rm":"rimraf build",
    "compile": "yarn ncc && yarn pkg"
  },
  "dependencies": {
    "next": "10.0.0",
    "react": "17.0.1",
    "react-dom": "17.0.1"
  },
  "devDependencies": {
    "@vercel/ncc": "^0.26.2",
    "cross-env": "^7.0.3",
    "pkg": "^4.4.9",
    "rimraf": "^3.0.2"
  }
}
